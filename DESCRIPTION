Package: inser
Title: Generate Fishing Gear Selectivity Indicator Sheets
Version: 0.1.0
Authors@R: c(
    person("Marie", "Morfin", , "first.last@example.com", role = c("aut", "cre"),
           comment = c(ORCID = "YOUR-ORCID-ID")),
    person("Swann", "Floc'hlay", , "swann@thinkr.fr", role = "aut",
